{
  "manifest_version": 2,

  "name": "Air Quality Egg Helper",
  "description": "Adds useful info to Air Quality Egg page on Cosm",
  "version": "0.1",

  "minimum_chrome_version" : "22.0.0.0",

  "icons": { "16": "images/icon16.png",
  	     "48": "images/icon48.png",
	     "128": "images/icon128.png"
	     },

  "web_accessible_resources": [
    "js/jquery-1.9.1.min.js",
    "js/aqehelper.js",
    "css/aqehelper.css",
    "images/icon128.png"
  ],
  "content_scripts" : [ {
    "matches": [ "http://airqualityegg.com/egg/*", "https://airqualityegg.com/egg/*" ],
    "js" : [ "js/jquery-1.9.1.min.js",  "js/aqehelper.js" ],
    "css" : [ "css/aqehelper.css" ]
    } ]
}
